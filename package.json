{
  "name": "weaver-ui-seed",
  "private": true,
  "version": "2.0.0",
  "description": "An example user interface using the Weaver-UI-Core",
  "repository": "https://github.com/TAMULib/Weaver-UI-Seed.git",
  "license": "MIT",
  "scripts": {
    "build:dev": "webpack",
    "build:watch": "webpack --watch",
    "build": "webpack -p",
    "prestart": "npm install -g grunt-cli",
    "postinstall": "npm run build",
    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js  --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js",
    "stop": "wvr server --stop",
    "start": "wvr server -b /cap --prod --uid magpie-webserver",
    "start-dev": "wvr server -b /magpie --uid magpie-webserver",
    "lint": "eslint \"app/**/*(*.js|*.ts)\"",
    "lint:fix": "eslint \"app/**/*(*.js|*.ts)\" --fix"
  },
  "dependencies": {
    "ng-table": "3.0.1",
    "ng-file-upload": "12.2.13",
    "weaver-ui-core": "git+https://github.com/TAMULib/Weaver-UI-Core.git#webpack-upgrade"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "angular": "^1.7.5",
    "babel-loader": "^8.0.4",
    "copy-webpack-plugin": "^4.6.0",
    "coveralls": "3.0.2",
    "css-loader": "2.1.0",
    "eslint": "^5.9.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-angular": "^4.0.0",
    "file-loader": "3.0.1",
    "html-loader": "^0.5.5",
    "karma": "3.1.4",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.1",
    "karma-jasmine": "2.0.1",
    "karma-junit-reporter": "1.2.0",
    "karma-ng-html2js-preprocessor": "1.0.0",
    "karma-webpack": "4.0.0-rc.3",
    "node-sass": "4.11.0",
    "protractor": "5.1.2",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "symlink-webpack-plugin": "^0.1.0",
    "url-loader": "1.1.2",
    "webpack": "4.28.1",
    "webpack-cli": "^3.1.2"
  }
}
