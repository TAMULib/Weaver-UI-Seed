{
  "name": "weaver-ui-seed",
  "private": true,
  "version": "2.0.0",
  "description": "An example user interface using the Weaver-UI-Core",
  "repository": "https://github.com/TAMULib/Weaver-UI-Seed.git",
  "license": "MIT",
  "main": "./app/app.js",
  "scripts": {
    "build:dev": "webpack",
    "build:watch": "webpack --watch",
    "build": "webpack -p",
    "prestart": "npm install -g grunt-cli",
    "postinstall": "npm run build",
    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js  --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js",
    "stop": "wvr server --stop",
    "start": "wvr server -b /cap --prod --uid magpie-webserver",
    "start-dev": "wvr server -b /magpie --uid magpie-webserver",
    "lint": "eslint \"app/**/*(*.js|*.ts)\"",
    "lint:fix": "eslint \"app/**/*(*.js|*.ts)\" --fix"
  },
  "dependencies": {
    "ng-table": "3.0.1",
    "ng-file-upload": "12.2.13",
    "weaver-ui-core": "git+https://github.com/TAMULib/Weaver-UI-Core.git#webpack-upgrade"
  },
  "devDependencies": {
  }
}
